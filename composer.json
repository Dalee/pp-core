{
	"name": "pp/core",
	"type": "pp-core",
	"description": "Proxima CMS",
	"authors": [
		{
			"name": "Dalee Dev Team",
			"email": "dev@dalee.ru"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.dev.dalee.ru"
		}
	],
	"config": {
		"secure-http": false,
		"platform": {
			"php": "5.4.22"
		}
	},
	"bin": [
		"bin/pp"
	],
	"require": {
		"pp/installers": "*",
		"monolog/monolog": "^1.19",
		"symfony/http-foundation": "~2.8",
		"ramsey/uuid": "^3.5",
		"symfony/console": "^2.8",
		"symfony/event-dispatcher": "^2.8",
		"symfony/yaml": "^2.8",
		"symfony/var-dumper": "^2.8",
		"vlucas/phpdotenv": "^2.4",
		"symfony/config": "^2.8",
		"symfony/dependency-injection": "^2.8"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8"
	},
	"autoload": {
		"psr-4": {
			"Tests\\": [
				"tests/"
			],
			"PP\\": [
				"src/"
			]
		}
	}
}
