{
	"name": "pp/core",
	"type": "pp-core",
	"description": "ProximaPortal",
	"license": "MIT",
	"minimum-stability": "dev",
	"authors": [
		{
			"name": "Dalee Developers Team",
			"email": "dev@dalee.ru"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.git.dalee.ru"
		}
	],
	"require": {
		"pp/installers": "*",
		"monolog/monolog": "^1.19",
		"symfony/http-foundation": "~2.8"
	},
	"config": {
		"secure-http": false,
		"preferred-install": {
			"pp/*": "source",
			"*": "dist"
		}
	},
	"autoload": {
		"psr-4": {
			"PP\\": [
				"src/"
			]
		}
	}
}
