{
	"name": "pp/core",
	"type": "pp-core",
	"description": "Proxima CMS",
	"authors": [
		{
			"name": "Dalee Dev Team",
			"email": "topdev@dalee.ru"
		}
	],
	"repositories": [
		{
			"type": "github",
			"url": "https://github.com/Dalee/pp-installers"
		}
	],
	"config": {
		"secure-http": false,
		"platform": {
			"php": "5.6.31"
		}
	},
	"bin": [
		"bin/pp"
	],
	"require": {
		"pp/installers": "*",
		"monolog/monolog": "^1.19",
		"symfony/http-foundation": "~3.3.0",
		"ramsey/uuid": "^3.5",
		"symfony/console": "~3.3.0",
		"symfony/event-dispatcher": "~3.3.0",
		"symfony/yaml": "~3.3.0",
		"vlucas/phpdotenv": "^2.4",
		"symfony/config": "~3.3.0",
		"symfony/dependency-injection": "~3.3.0",
		"symfony/finder": "~3.3.0",
		"symfony/cache": "~3.3.0",
		"predis/predis": "^1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8",
		"friendsofphp/php-cs-fixer": "^2.7"
	},
	"autoload": {
		"psr-4": {
			"Tests\\": [
				"tests/"
			],
			"PP\\": [
				"src/"
			]
		}
	}
}
