{
	"name": "dalee/pp-core",
	"type": "pp-core",
	"description": "Proxima CMS",
	"license": "MIT",
	"authors": [
		{
			"name": "Dalee Dev Team",
			"email": "topdev@dalee.ru"
		}
	],
	"config": {
		"platform": {
			"php": "7.2"
		}
	},
	"bin": [
		"bin/pp"
	],
	"require": {
		"php": ">=7.2",
		"dalee/pp-installers": "~1.0.5",
		"monolog/monolog": "^1.19",
		"symfony/http-foundation": "~3.4",
		"ramsey/uuid": "^3.5",
		"symfony/console": "~3.4",
		"symfony/event-dispatcher": "~3.4",
		"symfony/yaml": "~3.4",
		"vlucas/phpdotenv": "^2.4",
		"symfony/config": "~3.4",
		"symfony/dependency-injection": "~3.4",
		"symfony/finder": "~3.4",
		"symfony/cache": "~3.4",
		"predis/predis": "^1.1",
		"ext-json": "*",
		"ext-pcntl": "*",
		"ext-posix": "*",
		"ext-simplexml": "*",
		"ext-pgsql": "*",
		"ext-gd": "*",
		"ext-fileinfo": "*",
		"ext-dom": "*",
		"ext-xsl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8",
		"friendsofphp/php-cs-fixer": "^2.7"
	},
	"autoload": {
		"psr-4": {
			"Tests\\": [
				"tests/"
			],
			"PP\\": [
				"src/"
			]
		}
	}
}
